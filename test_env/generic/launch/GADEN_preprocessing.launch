<!-- Mission file for GADEN preprocessing
    ROS - KINETIC
-->

<launch>
    <arg name="gadenPATH" default="/media/Data1TB/GADEN" />
    <arg name="cfdPATH" default="/media/Data1TB/CFD" />
    <arg name="datasetWind" default="sim_1ms_80/generic" />
    <arg name="scenario" default="Generic" />
    
    <node pkg="gaden_preprocessing" type="preprocessing" name="preprocessing" output="screen" >
     
        #Cell size
        <param name="cell_size" value="2"/>    
        
        #CAD models of the enviroment (.stl)
        <param name="number_of_models" value="1"/>
        <param name="model_0" value="$(arg gadenPATH)/$(arg scenario)/cad_models/Generic_walls.stl"/>
                
        #CAD model of the outlets (.stl)
        <param name="number_of_outlet_models" value="2"/>
        <param name="outlets_model_0" value="$(arg gadenPATH)/$(arg scenario)/cad_models/Generic_outlet_v2.stl"/>
        <param name="outlets_model_1" value="$(arg gadenPATH)/$(arg scenario)/cad_models/Generic_walls.stl"/>
        
        #3D Location of a point in free-space
        <param name="empty_point_x" value="100"/>    ### (m)
        <param name="empty_point_y" value="100"/>    ### (m)
        <param name="empty_point_z" value="25"/>      ### (m)
            

        #Wind Data (the node will append _i.csv to the name that is specified here)
        <param name="wind_files" value="$(arg cfdPATH)/$(arg scenario)/paraview/$(arg datasetWind)"/>

        #Where to write the output files
        <param name="output_path" value="$(arg gadenPATH)/$(arg scenario)" />
    </node>
</launch>